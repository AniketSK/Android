        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="sampleapplication.parzival.com.kotlin.RecyclerViewKotlinActivity"></activity>
        <activity android:name="sampleapplication.parzival.com.java.RecyclerViewJavaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="sampleapplication.parzival.com.MainActivity">
        </activity>
    </application>

</manifest>